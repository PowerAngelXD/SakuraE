{
  "name": "sakurae",
  "displayName": "SakuraE",
  "description": "VS Code Support Extension for SakuraE",
  "version": "0.0.7",
  "engines": {
    "vscode": "^1.101.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/PowerAngelXD/SakuraE"
  },
  "contributes": {
    "languages": [{
      "id": "sak",
      "aliases": ["SakuraE", "sak"],
      "extensions": [".sak"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "sak",
      "scopeName": "source.sak",
      "path": "./syntaxes/sak.tmLanguage.json"
    }]
  }
}
